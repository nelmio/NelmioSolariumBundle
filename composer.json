{
    "name": "nelmio/solarium-bundle",
    "description": "Integration with solarium solr client.",
    "keywords": ["solr", "solarium", "search"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Symfony Community",
            "homepage": "https://github.com/nelmio/NelmioSolariumBundle/contributors"
        }
    ],
    "require": {
        "php": "^8.1",
        "solarium/solarium": "^6.4.1",
        "symfony/dependency-injection": "^6.4 || ^7.3 || ^8.0",
        "symfony/framework-bundle": "^6.4 || ^7.3 || ^8.0",
        "symfony/http-kernel": "^6.4 || ^7.3 || ^8.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^7.3 || ^8.0",
        "symfony/stopwatch": "^6.4 || ^7.3 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Nelmio\\SolariumBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
